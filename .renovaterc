{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "constraints": {
    "go": "1.25"
  },
  "extends": [
    "config:base",
    ":semanticCommitTypeAll(build)",
    ":separatePatchReleases"
  ],
  "ignorePresets": [
    ":combinePatchMinorReleases",
    ":prHourlyLimit2",
    ":semanticPrefixFixDepsChoreOthers"
  ],
  "enabledManagers": [
    "gomod"
  ],
  "postUpdateOptions": [
      "gomodTidy",
      "gomodMassage"
  ]
}
